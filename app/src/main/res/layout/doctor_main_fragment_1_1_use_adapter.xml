<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_fragment_1_1_use_adapter_background"
    android:orientation="vertical">

    <LinearLayout
    android:layout_width="match_parent"
    android:layout_height="40dp"
    android:orientation="horizontal">

        <TextView
            android:layout_width="142dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="编号"
            android:textColor="@color/text_blue" />

    <TextView
        android:id="@+id/yuyue_bianhao"
        android:layout_width="200dp"
        android:gravity="center"
        android:layout_height="match_parent"
        android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:layout_width="142dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="姓名"
            android:textColor="@color/text_blue" />

        <TextView
            android:id="@+id/yuyue_name"
            android:layout_width="200dp"
            android:gravity="center"
            android:layout_height="match_parent"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:textColor="@color/text_blue"
            android:gravity="center"
            android:layout_width="142dp"
            android:layout_height="match_parent"
            android:text="性别" />

        <TextView
            android:id="@+id/yuyue_sex"
            android:layout_width="200dp"
            android:gravity="center"
            android:layout_height="match_parent"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:textColor="@color/text_blue"
            android:gravity="center"
            android:layout_width="142dp"
            android:layout_height="match_parent"
            android:text="年龄" />

        <TextView
            android:id="@+id/yuyue_age"
            android:layout_width="200dp"
            android:gravity="center"
            android:layout_height="match_parent"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:textColor="@color/text_blue"
            android:gravity="center"
            android:layout_width="142dp"
            android:layout_height="match_parent"
            android:text="检查类型" />

        <TextView
            android:id="@+id/yuyue_type"
            android:layout_width="200dp"
            android:gravity="center"
            android:layout_height="match_parent"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:textColor="@color/text_blue"
            android:gravity="center"
            android:layout_width="142dp"
            android:layout_height="match_parent"
            android:text="检查部位" />

        <TextView
            android:id="@+id/yuyue_buwei"
            android:layout_width="200dp"
            android:gravity="center"
            android:layout_height="match_parent"
            android:text="TextView" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:textColor="@color/text_blue"
            android:gravity="center"
            android:layout_width="142dp"
            android:layout_height="match_parent"
            android:text="检查项目" />

        <TextView
            android:id="@+id/yuyue_xiangmu"
            android:layout_width="200dp"
            android:gravity="center"
            android:layout_height="match_parent"
            android:text="TextView" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

        <TextView
            android:textColor="@color/text_blue"
            android:gravity="center"
            android:layout_width="142dp"
            android:layout_height="match_parent"
            android:text="登记时间" />

        <TextView
            android:id="@+id/yuyue_time"
            android:layout_width="200dp"
            android:gravity="center"
            android:layout_height="match_parent"
            android:text="TextView" />
    </LinearLayout>



</LinearLayout>
